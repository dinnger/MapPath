{
  "name": "mappath",
  "displayName": "MapPath - Analizador visual de dependencias",
  "description": "Analizador visual de dependencias y estructura de proyectos",
  "version": "2.0.3",
  "engines": {
    "vscode": "^1.74.0"
  },
   "icon": "/media/icon.png",
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mappath.analyzeProject",
        "title": "Analizar proyecto",
        "category": "MapPath"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^5.45.0",
    "acorn": "^8.8.1",
    "acorn-walk": "^8.2.0"
  },
  "author": "dinnger",
  "license": "ISC",
  "publisher": "dinnger",
  "repository": {
    "type": "git",
    "url": "https://github.com/dinnger/MapPath"
  },
  "homepage": "https://github.com/dinnger/MapPath",
  "bugs": {
    "url": "https://github.com/dinnger/MapPath/issues"
  }
}
